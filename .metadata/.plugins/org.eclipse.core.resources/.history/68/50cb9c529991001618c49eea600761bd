
public class Main {

	public static void main(String[] args) {
		
		String parens = "(  ( ( ) ( ) )  )";
		int num = 0;
		
		MyStack stack = new MyStack();
		
		//remove spaces
		parens = parens.replaceAll("\\s", "");
		
		while(parens.isEmpty() == false)
		{
			
			if(parens.charAt(0) == '(')
			{
				stack.push('(');
				parens = parens.substring(1);
			}			
			else if(parens.charAt(0) == ')')
			{
				do
				{
					stack.pop();
					num++;
				}while(stack.peak() != '(' ); 

				parens = parens.substring(1);				
			}
						
		}
		
		
		
		
		
/**		
