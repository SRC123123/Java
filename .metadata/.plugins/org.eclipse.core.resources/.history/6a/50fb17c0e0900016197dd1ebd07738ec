
public class MyStack {
	
	private int data;
	private Node next;
	private Node prev;
	
	public MyStack()
	{
		
		prev = new Node();
		next = new Node();
		prev.next = next;
		next.prev = prev;		
	}
	
	public void push(int data)
	{
		prev.data = data;
		next.next = new Node();
		prev.next = next;
		next.prev = prev;
	}
	
	public void pop()
	{
		this.prev.next = null;
	}
	
	public int peak()
	{
		return this.data;
	}
	
	private class Node {
		private int data;
		private Node next;
		private Node prev;
	}

}
