
public class MyStack {
	
	private int data;
	private Node head;
	
	public MyStack()
	{
		head = new Node();	
	}
	
	public void push(int data)
	{
		head.data = data;
		head.next = new Node();
		head.prev = head;
		head = head.next;		
	}
	
	public void pop()
	{
		head.prev.next = null;
	}
	
	public int peak()
	{
		return this.data;
	}
	
	private class Node {
		private int data;
		private Node next;
		private Node prev;
	}

}
