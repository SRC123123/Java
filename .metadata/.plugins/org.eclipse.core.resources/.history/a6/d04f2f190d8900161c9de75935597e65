import java.io.File;
import java.io.FileNotFoundException;
import java.util.Random;
import java.util.Scanner;

public class Alg3 {

	public static void main(String[] args) throws FileNotFoundException {
		
		//Initialize
		int n = 10, k = 3, i = 0;
		long times[] = new long[10];
		Random gen = new Random();		
		gen.setSeed(System.nanoTime());
		Scanner fin = new Scanner(new File("input.txt"));
		//add date to header
		System.out.println("Algorithm 3: ");

		while(fin.hasNext())		
		{
			k = fin.nextInt();
			n = fin.nextInt();
			times[i] = System.nanoTime();
			Algorithm(gen, n, k);
			times[i] = System.nanoTime() - times[i];
			System.out.println("("+ n + ", " + k + "): " + times[i]);
			i++;
		}
		
		//Sanity Check
		int [] deck = new int [0-9];
		int [] hand = new int [5];
		int temp;
		
		for(i = 0; i < 5; i++)
		{
			System.out.print("\nSanity Check " + i +": ");
			int size = deck.length;
			for (int x =0; x < 5; x++, size--)
			     {
			          temp = gen.nextInt(size);
			          System.out.print(deck[temp] + ", ");
			          deck[temp] = deck[size-1];
			     }
		}
		
		
	}
	
private static void Algorithm(Random gen, int n, int k)
{
	
	int [] deck = new int [n];
	int [] hand = new int [k];
	int temp;
	
	int size = deck.length;
	for (int i =0; i < k; i++, size--)
	     {
	          temp = gen.nextInt(size);
	          hand[i] = deck[temp];
	          deck[temp] = deck[size-1];
	     }

	
}

}
