
public class MyStack {
	
	private Integer data;
	private MyStack next;
	
	public void push(int data)
	{
		this.data = data;
		this.next = next;		
	}
	
	public void pop()
	{
		MyStack tmp = new MyStack();
		tmp.next = this.next;
		this.data = this.next.data;
		this.next = this.next.next;
		tmp.next = null;
	}
	
	public int peak()
	{
		return this.data;
	}

}
