
public class MyStack {
	
	private int data;
	private Node next;
	private Node prev;
	
	public void push(int data)
	{
		this.prev = new Node();
		this.next = new Node();
		this.prev.next = next;
		this.next.prev = prev;
	}
	
	public void pop()
	{
		this.prev.next=null;
	}
	
	public int peak()
	{
		return this.data;
	}
	
	private class Node {
		private int data;
		private Node next;
		private Node prev;
	}

}
